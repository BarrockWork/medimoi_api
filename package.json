{
  "name": "medimoi-back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "dotenv -e .env.local -- npx nodemon ./bin/www",
    "status-migration": "dotenv -e .env.local -- npx prisma migrate status",
    "create-migration": "dotenv -e .env.local -- npx prisma migrate dev --create-only",
    "execute-migration": "dotenv -e .env.local -- npx prisma migrate dev",
    "reset-migration": "dotenv -e .env.local -- npx prisma migrate reset",
    
    "status-migration-prod": "npx prisma migrate status",
    "create-migration-prod": "npx prisma migrate dev --create-only",
    "execute-migration-prod": "npx prisma migrate dev",
    "reset-migration-prod": "npx prisma migrate reset",
    "start-prod": "npx nodemon ./bin/www",

    "deploy-migration": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^3.8.0",
    "axios": "^0.24.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^11.0.0",
    "dotenv-cli": "^4.1.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "ramda": "^0.27.2",
    "stripe": "^8.197.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "prisma": "^3.8.0"
  }
}

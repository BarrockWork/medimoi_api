{
  "name": "medimoi-back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "dotenv -e .env.local -- npx nodemon ./bin/www",
    "start-test": "dotenv -e .env.test -- npx nodemon ./bin/www",
    "status-migration": "dotenv -e .env.local -- npx prisma migrate status",
    "prismix": "npx prismix && prisma format",
    "create-migration": "dotenv -e .env.local -- npx prisma migrate dev --create-only",
    "execute-migration": "dotenv -e .env.local -- npx prisma migrate dev",
    "execute-migration-test": "dotenv -e .env.test -- npx prisma migrate dev",
    "reset-migration-test": "dotenv -e .env.test -- npx prisma migrate reset",
    "reset-migration": "dotenv -e .env.local -- npx prisma migrate reset",
    "status-migration-prod": "npx prisma migrate status",
    "create-migration-prod": "npx prisma migrate dev --create-only",
    "execute-migration-prod": "npx prisma migrate dev",
    "reset-migration-prod": "npx prisma migrate reset",
    "start-prod": "npx nodemon ./bin/www",
    "deploy-migration": "npx prisma migrate deploy",
    "apidoc-generate": "apidoc -i src/routes -o public/docs -c config/apidoc.json",
    "test": "dotenv -e .env.test -- jest ",
    "test-watch": "dotenv -e .env.test -- jest --watch",
    "test-coverage": "dotenv -e .env.test -- jest --coverage",
    "data-seed": "dotenv -e .env.local -- node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^3.8.0",
    "apidoc": "^0.50.3",
    "axios": "^0.24.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^11.0.0",
    "dotenv-cli": "^4.1.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "ramda": "^0.27.2",
    "slugify": "^1.6.5",
    "stripe": "^8.197.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.8.0",
    "prismix": "^1.0.21",
    "supertest": "^6.2.2"
  }
}
